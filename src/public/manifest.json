{
  "short_name": "React App",
  "manifest_version": 3,
  "name": "Train of Thought",
  "version": "0.0.2",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Train of Thought for Chrome"
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "tabs",
    "identity",
    "identity.email",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
